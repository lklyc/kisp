<%= render 'shared/header', header: 'New Concept', subtitle: nil %>
<%= render 'shared/errors', obj: @concept %>

<br/>
<div class="container" id="container-forms">
  <%= form_for @concept do |f| %>
    <div class="form-group">
      <%= f.label :concept %>
      <%= f.text_field :concept, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :context %>
      <%= f.text_field :context, class: "form-control" %>
    </div>      
    <div class="form-group">
      <%= f.submit 'Create', class: 'btn btn-success'%>
    </div>
  <% end %>
</div>